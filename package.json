{
  "name": "tetris-game2",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "bun@1.2.18",
  "license": "ISC",
  "sideEffects": [
    "*.css",
    "./src/index.css",
    "./src/styles/*.css",
    "./src/i18n/config.ts"
  ],
  "scripts": {
    "dev": "bun --bun vite",
    "build": "bun x vite build",
    "test": "bun test src/ --ignore='**/benchmarks/**' --ignore='**/visual/**' --ignore='tests/**' --ignore='**/components/**'",
    "test:full": "bun test src/ --ignore='**/benchmarks/**' --ignore='**/visual/**' --ignore='tests/**' --ignore='**/components/**'",
    "test:a11y": "bun x playwright test tests/accessibility.spec.ts",
    "lint": "bun x biome check --write",
    "format": "bun x biome format --write",
    "typecheck": "bun x tsc --noEmit",
    "preview": "bun x vite preview",
    "ci": "bun x biome ci && bun run typecheck && bun test && bun run build",
    "prepare": "lefthook install",
    "check:i18n": "bun run scripts/check-i18n-keys.ts",
    "audit:accessibility": "bun run scripts/accessibility-audit.ts",
    "audit:accessibility:comprehensive": "bun run scripts/accessibility-audit.ts --comprehensive",
    "e2e": "bun x playwright test --config=playwright.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "bun run scripts/analyze-bundle.ts",
    "analyze:visual": "bun x vite build && bun x rollup-plugin-visualizer dist/stats.html"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "25.3.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "15.6.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@biomejs/biome": "2.1.1",
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/react": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.18",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-oxc": "^0.2.3",
    "chromatic": "^13.1.2",
    "fast-check": "^4.2.0",
    "happy-dom": "^18.0.1",
    "knip": "^5.61.3",
    "lefthook": "1.12.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.0.16",
    "tailwindcss": "4.1.11",
    "terser": "^5.43.1",
    "typescript": "5.8.3",
    "vite": "npm:rolldown-vite@^7.0.9",
    "vitest": "^2.1.9"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "lefthook"
  ]
}
